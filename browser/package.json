{
    "name": "combocss-browser",
    "version": "0.6.6",
    "description": "Browser version of ComboCSS a CSS Toolkit for combining CSS properties",
    "keywords": [
        "conbocss",
        "postcss",
        "css"
    ],
    "scripts": {
        "test": "node index.test.js"
    },
    "author": "Benjamin Lotterer <theedgesofben@hotmail.com>",
    "license": "MIT",
    "repository": "theedgesofben/combocss",
    "engines": {
        "node": ">=16.0.0"
    },
    "peerDependencies": {
        "postcss": "^8.3.0"
    },
    "eslintConfig": {
        "parserOptions": {
            "ecmaVersion": "latest"
        },
        "env": {
            "node": true,
            "es6": true
        },
        "extends": [
            "eslint:recommended",
            "plugin:jest/recommended"
        ],
        "rules": {
            "jest/expect-expect": "off"
        }
    },
    "jest": {
        "coverageThreshold": {
            "global": {
                "statements": 100
            }
        },
        "transform": {}
    },
    "dependencies": {
        "autoprefixer": "^10.4.17",
        "parse5": "^6.0.1",
        "postcss": "^8.4.6"
    },
    "browserslist": [
        "last 15 version",
        "not dead"
    ],
    "type": "module",
    "main": "browser.js"
}
